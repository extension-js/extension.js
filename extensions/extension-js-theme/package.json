{
  "type": "module",
  "name": "@extension-js/theme",
  "private": true,
  "version": "0.1.0",
  "description": "Chrome theme inspired by GitHub Dark, adapted to Apple HIG dark palette with accent #1E76E6.",
  "license": "MIT",
  "scripts": {
    "build:chromium": "pnpm -s exec extension build --browser=chromium",
    "build:chrome": "pnpm -s exec extension build --browser=chrome",
    "build:firefox": "pnpm -s exec extension build --browser=firefox",
    "build:edge": "pnpm -s exec extension build --browser=edge"
  },
  "keywords": [
    "chrome",
    "theme",
    "dark"
  ]
}
